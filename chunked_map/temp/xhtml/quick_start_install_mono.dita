                        <uicontrol class="+ topic/ph ui-d/uicontrol " xtrf="/Users/isaaceldridge/Desktop/wip/DITA_sandbox/source /quick_start_install_mono.dita" xtrc="uicontrol:5;108:36">Install PostgreSQL for me</uicontrol>.</cmd>
