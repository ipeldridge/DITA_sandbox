                <substeps id="substeps_v5t_s44_4q" class="- topic/ol task/substeps " xtrf="/Users/isaaceldridge/Desktop/wip/DITA_sandbox/source /quick_start_install_mono.dita" xtrc="substeps:1;92:52">
                    <substep class="- topic/li task/substep " xtrf="/Users/isaaceldridge/Desktop/wip/DITA_sandbox/source /quick_start_install_mono.dita" xtrc="substep:1;93:30">
