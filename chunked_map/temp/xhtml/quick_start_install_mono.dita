                    user:</cmd>
                <substeps id="substeps_sfg_hp4_4q" class="- topic/ol task/substeps " xtrf="/Users/isaaceldridge/Desktop/wip/DITA_sandbox/source /quick_start_install_mono.dita" xtrc="substeps:2;113:52">
                    <substep class="- topic/li task/substep " xtrf="/Users/isaaceldridge/Desktop/wip/DITA_sandbox/source /quick_start_install_mono.dita" xtrc="substep:3;114:30">
